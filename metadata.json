{
  "name": "juniorsysadmin-irqbalance",
  "version": "1.0.4",
  "author": "Ben Spiccia",
  "summary": "Puppet module for installing and managing irqbalance",
  "license": "Apache-2.0",
  "source": "git://github.com/juniorsysadmin/puppet-irqbalance.git",
  "project_page": "https://github.com/juniorsysadmin/puppet-irqbalance",
  "issues_url": "https://github.com/juniorsysadmin/puppet-irqbalance/issues",
  "tags": ["irqbalance", "irq"],
  "operatingsystem_support": [
    {
